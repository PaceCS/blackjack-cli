{
  "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
  },
  "extends": "airbnb/base",
  "rules": {
    "indent": ["warn", 4],
    "prefer-template": "off",
    "eqeqeq": "off",
    "no-console": "off",
    "no-unused-vars": "off",
    "radix": "off",
    "no-multiple-empty-lines": "off",
    "no-await-in-loop": "off"
  }
}
